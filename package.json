{
  "name": "srs",
  "displayName": "SRS",
  "description": "Syntex repository services VS Code Extension ",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.81.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:srs.generateCertificate"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "syntex-repository-services",
          "title": "Syntex repository services",
          "icon": "media/container.svg"
        }
      ]
    },
    "views": {
      "syntex-repository-services": [
        {
          "id": "syntex-repository-services",
          "name": "Syntex repository services"
        }
      ],
      "new-view": [
        {
          "id": "new-view",
          "name": "New View"
        }
      ]
    },
    "viewsWelcome": [
      {
        "view": "syntex-repository-services",
        "contents": "Welcome to Syntex Repository Services! To begin, please log in with your partner tenant credentials below: \n[Log In](command:srs.login)\n"
      }
    ],
    "commands": [
      {
        "command": "srs.generateCertificate",
        "title": "Syntex repository services: Generate Certificate"
      },
      {
        "command": "srs.uploadCertificate",
        "title": "Syntex repository services: Upload Certificate"
      },
      {
        "command": "srs.login",
        "title": "Syntex repository services: Login to AAD"
      },
      {
        "command": "srs.createNewContainerTypeCommand",
        "title": "Syntex repository services: Create New ContainerType"
      },
      {
        "command": "srs.getFileStorageContainerToken",
        "title": "Syntex repository services: Get FileStorageContainer Token"
      },
      {
        "command": "srs.callMSGraphCommand",
        "title": "Syntex repository services: Call MSGraph"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/jsrsasign": "^10.5.8",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/node-forge": "^1.3.4",
    "@types/vscode": "^1.81.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vscode/test-electron": "^2.3.2",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.3.0-dev.20230825"
  },
  "dependencies": {
    "@azure/msal-node": "^2.0.1",
    "axios": "^1.4.0",
    "jsrsasign": "^10.8.6",
    "jsrsasign-util": "^1.0.5",
    "msal-node": "^0.0.1-security",
    "node-forge": "^1.3.1",
    "uuid": "^9.0.0"
  }
}
